FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];  // Temperature: K

internalField   uniform 300;  // Room temperature (27Â°C)

boundaryField
{
    walls
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    
    beamWalls
    {
        type            zeroGradient;
    }
    
    nozzle
    {
        type            zeroGradient;
    }
    
    nozzleInlet
    {
        type            fixedValue;
        value           uniform 300;
    }
    
    beamFrontOpening
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    
    beamBackOpening
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    
    beamHoles
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    
    enclosure
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
}

